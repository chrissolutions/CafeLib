<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageIconUrl></PackageIconUrl>
    <Description>Cafe library for Mobile applications.</Description>
    <AssemblyVersion>0.8.0.0</AssemblyVersion>
    <Version>0.8.0</Version>
    <PackageIcon>cafelib.png</PackageIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Chris Solutions</Authors>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="CafeLib.Core" Version="0.8.0" />
    <PackageReference Include="CafeLib.Core.Collections" Version="0.8.0" />
    <PackageReference Include="CafeLib.Core.Eventing" Version="0.8.0" />
    <PackageReference Include="CafeLib.Core.IoC" Version="0.8.0" />
    <PackageReference Include="Xamarin.Forms" Version="4.7.0.1080" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="..\..\cafelib.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>
