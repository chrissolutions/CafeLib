<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageIconUrl></PackageIconUrl>
    <Description>Cafe library for Mobile applications.</Description>
    <AssemblyVersion>0.7.5.0</AssemblyVersion>
    <Version>0.7.5</Version>
    <PackageIcon>cafelib.png</PackageIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>Chris Solutions</Company>
    <Authors>Chris Solutions</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CafeLib.Core" Version="0.7.5" />
    <PackageReference Include="CafeLib.Core.Caching" Version="0.7.5" />
    <PackageReference Include="CafeLib.Core.Eventing" Version="0.7.5" />
    <PackageReference Include="CafeLib.Core.IoC" Version="0.7.5" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.0.0" />
    <PackageReference Include="Xamarin.Forms" Version="4.3.0.947036" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\cafelib.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
