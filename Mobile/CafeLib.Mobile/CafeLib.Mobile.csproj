<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageIconUrl>https://raw.githubusercontent.com/cwaldron/CafeLib/master/favicon.ico</PackageIconUrl>
    <Description>Cafe library for Mobile applications.</Description>
    <AssemblyVersion>0.7.1.0</AssemblyVersion>
    <Version>0.7.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.0.0" />
    <PackageReference Include="Xamarin.Forms" Version="4.3.0.908675" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="CafeLib.Core">
      <HintPath>..\..\Core\CafeLib.Core.IoC\bin\Debug\netstandard2.0\CafeLib.Core.dll</HintPath>
    </Reference>
    <Reference Include="CafeLib.Core.Eventing">
      <HintPath>..\..\Core\CafeLib.Core.IoC\bin\Debug\netstandard2.0\CafeLib.Core.Eventing.dll</HintPath>
    </Reference>
    <Reference Include="CafeLib.Core.IoC">
      <HintPath>..\..\Core\CafeLib.Core.IoC\bin\Debug\netstandard2.0\CafeLib.Core.IoC.dll</HintPath>
    </Reference>
    <Reference Include="CafeLib.Core.Logging">
      <HintPath>..\..\Core\CafeLib.Core.Logging\bin\Debug\netstandard2.0\CafeLib.Core.Logging.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
