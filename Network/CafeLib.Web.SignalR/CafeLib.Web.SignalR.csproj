<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<Description>Cafelib Signal/R support package supporting method binding.</Description>
		<Version>0.9.0</Version>
		<Copyright></Copyright>
		<PackageReleaseNotes></PackageReleaseNotes>
		<PackageIcon>cafelib.png</PackageIcon>
		<AssemblyVersion>0.9.0.0</AssemblyVersion>
		<FileVersion>0.9.0.0</FileVersion>
		<PackageIconUrl />
		<Authors>Chris Solutions</Authors>
		<LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CafeLib.Core.Logging" Version="0.9.0" />
    <PackageReference Include="CafeLib.Core.MethodBinding" Version="0.9.0" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="3.1.9" />
    <PackageReference Include="CafeLib.Core" Version="0.9.0" />
    <PackageReference Include="CafeLib.Core.Runnable" Version="0.9.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="..\..\cafelib.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>
