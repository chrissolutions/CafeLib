<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.9.0</Version>
    <Description>Authorization identity model.</Description>
    <PackageIcon>cafelib.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="IdentityDb.sql" />
    <None Include="..\..\cafelib.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="IdentityDb.sql" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CafeLib.Core.Hashing" Version="0.9.0" />
    <PackageReference Include="CafeLib.Data" Version="0.9.0" />
  </ItemGroup>

</Project>
